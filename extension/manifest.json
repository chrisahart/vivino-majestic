{
    "manifest_version": 3,
    "name": "Your Extension Name",
    "version": "1.0",
    "content_scripts": [
        {
          "matches": ["*://www.majestic.co.uk/*"],
          "js": ["content.js"]
        }
      ],
      "permissions": ["storage", "activeTab"],
      "host_permissions": [
        "*://www.majestic.co.uk/*",
        "*://www.vivino.com/*"
      ],
      "background": {
        "service_worker": "service-worker.js",
        "type": "module"
      }
  }
  